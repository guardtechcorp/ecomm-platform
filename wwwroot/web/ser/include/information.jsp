<% if (sDisplayMessage!=null) { %>
<table width="100%" align="center" border="0" cellpadding="0" cellspacing="0">
<tr>
 <td>
    <table width="100%" align="center" border="0" cellpadding="5" cellspacing="1" style="border: 1px solid #DFDFDF; background-color:#EFF7FE">
     <tr>
       <td colspan="2" align="center"><font size="2" face="Verdana, Arial, Helvetica, sans-serif" color='<%=(nDisplay==1)?"green":"#FF6633"%>'>
        <b><%=sDisplayMessage%></b></font>
       </td>
     </tr>
    </table>
  </td>
 </tr>
 <tr>
   <td height="10"></td>
 </tr>
</table>
<% } %>