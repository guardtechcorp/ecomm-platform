   <load-on-startup>1</load-on-startup>
</servlet-->

   <servlet>
      <servlet-name>adsdeliver</servlet-name>
      <servlet-class>com.zyzit.weboffice.servlet.AdsServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>adsdeliver</servlet-name>
      <url-pattern>/service/ads/*</url-pattern>
   </servlet-mapping>
  <servlet>
    <servlet-name>chart</servlet-name>
    <servlet-class>com.zyzit.weboffice.servlet.SimpleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dispatcher</servlet-name>
    <servlet-class>com.zyzit.weboffice.servlet.Dispatcher</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>fileload</servlet-name>
    <servlet-class>com.zyzit.weboffice.servlet.LoadFile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>startup</servlet-name>
    <jsp-file>/admin/start.jsp</jsp-file>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.zyzit.weboffice.servlet.Test</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>transfer</servlet-name>
    <servlet-class>com.zyzit.weboffice.servlet.Transfer</servlet-class>
  </servlet>
  <servlet>
     <servlet-name>paypalcallback</servlet-name>
     <servlet-class>com.zyzit.weboffice.servlet.PaypalCallback</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>startup</servlet-name>
    <url-pattern>/startup.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dispatcher</servlet-name>
    <url-pattern>/admin/admin/request.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>transfer</servlet-name>
    <url-pattern>/admin/membership/register.asp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>chart</servlet-name>
    <url-pattern>/chart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dispatcher</servlet-name>
    <url-pattern>/web/request.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
   <servlet-name>fileload</servlet-name>
   <url-pattern>/web/fileload/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>paypalcallback</servlet-name>
    <url-pattern>/web/paypalcallback</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/admin/admin/index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
